<taskModel xmlns="http://www.cs.wpi.edu/~rich/cetask/cea-2018-ext" xmlns:std="urn:disco.wpi.edu:htnlfd:std" about="urn:disco.wpi.edu:htnlfd:setTable1">
  <task id="PlaceSilverware">
    <input name="input1" type="boolean"/>
    <subtasks id="placeSilverware1">
      <step name="place1" task="std:Place"/>
      <step name="place2" task="std:Place"/>
      <step name="place3" task="std:Place"/>
      <applicable>$this.input1</applicable>
      <binding slot="$place1.referenceFrame" value="Object.DinnerPlate"/>
      <binding slot="$place3.referenceFrame" value="Object.DinnerPlate"/>
      <binding slot="$place1.object" value="Object.Knife"/>
      <binding slot="$place2.referenceFrame" value="Object.DinnerPlate"/>
      <binding slot="$place3.object" value="Object.Spoon"/>
      <binding slot="$place2.object" value="Object.Fork"/>
    </subtasks>
    <subtasks id="placeSilverware2">
      <step name="place1" task="std:Place"/>
      <step name="place2" task="std:Place"/>
      <applicable>!$this.input1</applicable>
      <binding slot="$place1.referenceFrame" value="Object.DinnerPlate"/>
      <binding slot="$place1.object" value="Object.Fork"/>
      <binding slot="$place2.referenceFrame" value="Object.DinnerPlate"/>
      <binding slot="$place2.object" value="Object.Spoon"/>
    </subtasks>
  </task>
  <task id="SetTable">
    <subtasks id="setTable1">
      <step name="placeGlasses1" task="PlaceGlasses"/>
      <step name="placeSilverware1" task="PlaceSilverware"/>
      <step name="placeDishes1" task="PlaceDishes"/>
    </subtasks>
  </task>
  <task id="PlaceGlasses">
    <input name="input1" type="boolean"/>
    <subtasks id="placeGlasses1">
      <step name="place1" task="std:Place"/>
      <step name="place2" task="std:Place"/>
      <applicable>$this.input1</applicable>
      <binding slot="$place1.referenceFrame" value="Object.DinnerPlate"/>
      <binding slot="$place1.object" value="Object.WineGlass"/>
      <binding slot="$place2.referenceFrame" value="Object.DinnerPlate"/>
      <binding slot="$place2.object" value="Object.WaterGlass"/>
    </subtasks>
    <subtasks id="placeGlasses2">
      <step name="place1" task="std:Place"/>
      <applicable>!$this.input1</applicable>
      <binding slot="$place1.referenceFrame" value="Object.DinnerPlate"/>
      <binding slot="$place1.object" value="Object.WaterGlass"/>
    </subtasks>
  </task>
  <task id="PlaceDishes">
    <input name="input1" type="boolean"/>
    <subtasks id="placeDishes1">
      <step name="place1" task="std:Place"/>
      <step name="place2" task="std:Place"/>
      <step name="place3" task="std:Place"/>
      <applicable>$this.input1</applicable>
      <binding slot="$place3.referenceFrame" value="Object.SquareTable"/>
      <binding slot="$place1.referenceFrame" value="Object.DinnerPlate"/>
      <binding slot="$place1.object" value="Object.SoupBowl"/>
      <binding slot="$place2.referenceFrame" value="Object.DinnerPlate"/>
      <binding slot="$place3.object" value="Object.DinnerPlate"/>
      <binding slot="$place2.object" value="Object.SaladPlate"/>
    </subtasks>
    <subtasks id="placeDishes2">
      <step name="place1" task="std:Place"/>
      <step name="place2" task="std:Place"/>
      <applicable>!$this.input1</applicable>
      <binding slot="$place1.referenceFrame" value="Object.DinnerPlate"/>
      <binding slot="$place1.object" value="Object.SoupBowl"/>
      <binding slot="$place2.referenceFrame" value="Object.SquareTable"/>
      <binding slot="$place2.object" value="Object.DinnerPlate"/>
    </subtasks>
  </task>
</taskModel>
