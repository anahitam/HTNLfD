<taskModel xmlns="http://www.cs.wpi.edu/~rich/cetask/cea-2018-ext" xmlns:setTable1="urn:disco.wpi.edu:htnlfd:setTable1" xmlns:std="urn:disco.wpi.edu:htnlfd:std" about="urn:disco.wpi.edu:htnlfd:setTable1">
  <task id="PlaceSilverware">
    <input name="referenceFrame1" type="PhysObj"/>
    <input modified="newObject1" name="object1" type="PhysObj"/>
    <input modified="newObject2" name="object2" type="PhysObj"/>
    <input modified="newObject3" name="object3" type="PhysObj"/>
    <input name="input1" type="boolean"/>
    <output name="newObject1" type="PhysObj"/>
    <output name="newObject2" type="PhysObj"/>
    <output name="newObject3" type="PhysObj"/>
    <subtasks id="placeSilverware1">
      <step name="place1" task="std:Place"/>
      <step name="place2" task="std:Place"/>
      <step name="place3" task="std:Place"/>
      <applicable>$this.input1</applicable>
      <binding slot="$this.newObject3" value="$place3.newObject"/>
      <binding slot="$place1.referenceFrame" value="$this.referenceFrame1"/>
      <binding slot="$place3.referenceFrame" value="$this.referenceFrame1"/>
      <binding slot="$place1.object" value="$this.object1"/>
      <binding slot="$this.newObject2" value="$place2.newObject"/>
      <binding slot="$this.newObject1" value="$place1.newObject"/>
      <binding slot="$place2.referenceFrame" value="$this.referenceFrame1"/>
      <binding slot="$place3.object" value="$this.object3"/>
      <binding slot="$place2.object" value="$this.object2"/>
    </subtasks>
    <subtasks id="placeSilverware11">
      <step name="place1" task="std:Place"/>
      <step name="place2" task="std:Place"/>
      <applicable>!$this.input1</applicable>
      <binding slot="$place1.referenceFrame" value="$this.referenceFrame1"/>
      <binding slot="$place1.object" value="$this.object1"/>
      <binding slot="$this.newObject2" value="$place2.newObject"/>
      <binding slot="$this.newObject1" value="$place1.newObject"/>
      <binding slot="$place2.referenceFrame" value="$this.referenceFrame1"/>
      <binding slot="$this.object2" value="PhysObj.Fork"/>
      <binding slot="$place2.object" value="$this.object2"/>
      <binding slot="$this.object1" value="PhysObj.Spoon"/>
      <binding slot="$this.referenceFrame1" value="PhysObj.DinnerPlate"/>
    </subtasks>
  </task>
